name: TeasePOV
sceneByURL:
  - action: scrapeXPath
    url:
      - teasepov.com
    scraper: sceneScraper
xPathScrapers:
  sceneScraper:
    scene:
      Title: //div[@class="slideTitle"]
      Date:
        selector: //div[@class="slideDate"]
        postProcess:
          - replace:
              - regex: "/"
                with: ""
          - parseDate: January 2, 2006
      Details:
        selector: //div[@class="videoDescription"]
      Studio:
        Name:
          fixed: TeasePOV
      Image:
        selector: //div[@id="player"]/img/@src    
